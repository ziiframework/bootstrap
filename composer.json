{
    "name": "ziiframework/bootstrap",
    "description": "zii-framework bootstrap",
    "keywords": ["zii-framework", "bootstrap"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/ziiframework/bootstrap/issues",
        "source": "https://github.com/ziiframework/bootstrap"
    },
    "authors": [
        {
            "name": "Yii Team",
            "email": "team@yiiframework.com",
            "homepage": "http://www.yiiframework.com/team",
            "role": "Founder and project lead"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {},
    "require-dev": {
        "ziiframework/zii": "~3.1.4",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "yii\\bootstrap\\": "src"
        }
    },
    "scripts": {
        "redo-install": [
            "rm -rf vendor/",
            "rm -f composer.lock",
            "@composer self-update -vvv",
            "@composer install -vvv"
        ]
    },
    "config": {
        "platform": {"php": "7.3"},
        "process-timeout": 1800
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
