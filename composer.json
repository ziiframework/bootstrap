{
    "name": "ziiframework/bootstrap",
    "description": "zii-framework bootstrap",
    "keywords": ["zii-framework", "bootstrap"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/ziiframework/bootstrap/issues",
        "source": "https://github.com/ziiframework/bootstrap"
    },
    "authors": [
        {
            "name": "Yii Team",
            "email": "team@yiiframework.com",
            "homepage": "http://www.yiiframework.com/team",
            "role": "Founder and project lead"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "ziiframework/zii": "~3.5.2",
        "bower-asset/bootstrap": "3.4.* | 3.3.* | 3.2.* | 3.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.cn"
        }
    ],
    "autoload": {
        "psr-4": {
            "yii\\bootstrap\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
